{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 22,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "conf-common",
      "description": "",
      "hidden": true,
      "generator": "Ninja",
      "installDir": "${sourceDir}/project/addons/steikemann2d"
    },
    {
      "name": "gcc-debug",
      "displayName": "gcc Debug",
      "description": "",
      "inherits": "conf-common",
      "cacheVariables": {
        "CMAKE_C_COMPILER": "gcc",
        "CMAKE_CXX_COMPILER": "g++",
        "CMAKE_BUILD_TYPE": "Debug"
      },
      "binaryDir": "${sourceDir}/build/${presetName}"
    },
    {
      "name": "gcc-release",
      "displayName": "gcc Release",
      "description": "",
      "inherits": "conf-common",
      "cacheVariables": {
        "CMAKE_C_COMPILER": "gcc",
        "CMAKE_CXX_COMPILER": "g++",
        "CMAKE_BUILD_TYPE": "Release"
      },
      "binaryDir": "${sourceDir}/build/${presetName}"
    },
    {
      "name": "mingw-debug",
      "displayName": "mingw Debug",
      "description": "",
      "inherits": "conf-common",
      "toolchainFile": "${sourceDir}/mingw-toolchain.cmake",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      },
      "binaryDir": "${sourceDir}/build/${presetName}"
    },
    {
      "name": "mingw-release",
      "displayName": "mingw Release",
      "description": "",
      "inherits": "conf-common",
      "toolchainFile": "${sourceDir}/mingw-toolchain.cmake",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      },
      "binaryDir": "${sourceDir}/build/${presetName}"
    }
  ]
}
