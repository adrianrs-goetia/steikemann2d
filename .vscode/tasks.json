{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "cmake build",
            "type": "shell",
            "command": "${workspaceFolder}/set_windows_env.bat",
            "args": [
                "cmake",
                "--build", "${workspaceFolder}/build",
                "--config", "Debug",
                "--target", "all"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": "myenv",
        },
    ]
}